{
  "name": "js-dev-env",
  "version": "1.0.1-build.3",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "// Essential Commands": "# Core commands for daily development",
    "dev": "npm-run-all copy-icons build-css-dev --parallel watch-css start:server",
    "build": "node tools/build/build.js",
    "test": "jest",
    "start": "npm run build && node tools/build/start.js",
    "// Development": "# Development workflow commands",
    "start:server": "nodemon index.js",
    "start:dev": "npm run dev",
    "watch-css": "sass --watch --load-path=node_modules scss/main.scss:public/css/styles.css",
    "// Build System": "# Build and deployment",
    "prebuild": "npm run clean",
    "build:dev": "npm run build-css-dev && npm run copy-icons",
    "build:prod": "npm run build",
    "build:github-pages": "npm run build",
    "clean": "node tools/build/clean.js",
    "// Testing": "# Test commands",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "// Quality": "# Code quality and linting",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "// Security": "# Security and auditing",
    "security:audit": "node tools/maintenance/security-audit.js",
    "audit:seo": "node tools/seo/seo-a11y-checks.mjs",
    "audit:ssl": "tsx tools/seo/ssl-expiry.ts",
    "audit:all": "npm run audit:seo && npm run audit:ssl",
    "// Version Management": "# Version control",
    "version:info": "node tools/maintenance/version.js info",
    "version:patch": "node tools/maintenance/version.js increment patch",
    "version:minor": "node tools/maintenance/version.js increment minor",
    "version:major": "node tools/maintenance/version.js increment major",
    "version:clean": "node tools/maintenance/version.js clean",
    "// Docker": "# Container commands",
    "docker:build": "docker build -t js-dev-env .",
    "docker:run": "docker run -p 3000:3000 js-dev-env",
    "docker:dev": "docker-compose up --build",
    "// Setup & Configuration": "# Project setup helpers",
    "setup:minimal": "node tools/maintenance/minimal-setup.js",
    "setup:interactive": "node tools/setup/interactive-setup.js",
    "features": "node tools/setup/feature-manager.js",
    "features:list": "node tools/setup/feature-manager.js list",
    "features:enabled": "node tools/setup/feature-manager.js enabled",
    "features:presets": "node tools/setup/feature-manager.js presets",
    "configure": "node tools/maintenance/configure-project.js",
    "quickstart": "echo 'ðŸš€ Quick Start: 1. npm install, 2. npm run dev, 3. Open http://localhost:3000'",
    "// Plugin Management": "# Plugin system commands",
    "plugins": "node plugins/plugin-cli.js",
    "plugins:list": "node plugins/plugin-cli.js list",
    "plugins:info": "node plugins/plugin-cli.js info",
    "plugins:create": "node plugins/plugin-cli.js create",
    "plugins:run": "node plugins/plugin-cli.js run",
    "// Template Management": "# Template generation commands",
    "templates": "node templates/template-cli.js",
    "templates:list": "node templates/template-cli.js list",
    "templates:info": "node templates/template-cli.js info",
    "templates:generate": "node templates/template-cli.js generate",
    "templates:wizard": "node templates/template-cli.js wizard",
    "templates:customize": "node templates/customization-wizard.js",
    "// Internal Tools": "# Usually not called directly",
    "build-css": "sass --load-path=node_modules scss/main.scss docs/css/styles.css",
    "build-css-dev": "sass --load-path=node_modules scss/main.scss public/css/styles.css",
    "copy-icons": "node tools/build/copy-icons.js",
    "copy-icons-docs": "node tools/build/copy-icons.js --target=docs",
    "copy-static-assets": "node tools/build/copy-static-assets.js",
    "generate-static-site": "node tools/build/generate-static-site.js",
    "build:pug": "node tools/build/build.js --pug",
    "build:scss": "node tools/build/build.js --scss",
    "// Analysis & Reports": "# Analysis and reporting",
    "analyze:deps": "node tools/maintenance/optimize-dependencies.js",
    "analyze:performance": "node tools/build/performance-analyzer.js analyze",
    "analyze:bundles": "node tools/build/performance-analyzer.js bundle",
    "performance:report": "node tools/build/performance-analyzer.js report",
    "performance:monitor": "node tools/build/performance-analyzer.js monitor",
    "report:monthly": "node tools/maintenance/report-monthly.mjs",
    "report:git": "node tools/git/report-git-simple.mjs",
    "// Maintenance": "# Maintenance and optimization",
    "optimize:deps": "node tools/maintenance/optimize-dependencies.js --generate-minimal",
    "test:deps": "node tools/maintenance/test-dependencies.js",
    "fix:auto": "node tools/maintenance/apply-autofixes.mjs",
    "// Help": "# Show available commands",
    "help": "echo 'ðŸ“š Essential Commands:' && echo 'npm run dev - Start development' && echo 'npm run build - Build for production' && echo 'npm run test - Run tests' && echo 'npm run docker:dev - Run with Docker' && echo '' && echo 'Use npm run <command> to see all commands in a category'"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@parcel/watcher": "^2.5.1",
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "compression": "^1.8.1",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "validator": "^13.15.15"
  }
}
